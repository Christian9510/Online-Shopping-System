<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="366.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.Controller">
   <children>
      <TextField fx:id="txtUsername" layoutX="33.0" layoutY="14.0" prefHeight="42.0" prefWidth="302.0" promptText="Username" />
      <PasswordField fx:id="txtPassword" layoutX="33.0" layoutY="66.0" prefHeight="42.0" prefWidth="302.0" promptText="Password" />
      <Button fx:id="btnLogin" layoutX="206.0" layoutY="118.0" mnemonicParsing="false" onAction="#login" prefHeight="42.0" prefWidth="105.0" text="Login" />
      <Button fx:id="btnCancel" layoutX="132.0" layoutY="172.0" mnemonicParsing="false" onAction="#cancelLogin" prefHeight="42.0" prefWidth="105.0" text="Cancel" />
      <Button layoutX="55.0" layoutY="118.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="105.0" text="Create Account" />
   </children>
</AnchorPane>
